{
  "cli": {
    "version": ">= 16.19.3",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "API_BASE_URL": "$(API_BASE_URL)"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "API_BASE_URL": "$(API_BASE_URL)"
      }
    },
    "production": {
      "autoIncrement": true,
      "distribution": "internal",
      "env": {
        "API_BASE_URL": "$(API_BASE_URL)"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
